{
  "name": "awesome-devsecops",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:security": "eslint src --ext .ts,.tsx --config .eslintrc.security.json",
    "lint:security:report": "eslint src --ext .ts,.tsx --config .eslintrc.security.json --format json --output-file eslint-report.json 2>/dev/null || true && node scripts/convert-eslint-to-sarif.js",
    "export": "next export",
    "validate-data": "node scripts/validate-data.js",
    "type-check": "tsc --noEmit --strict",
    "security-audit": "npm audit --audit-level=moderate"
  },
  "dependencies": {
    "react": "^18",
    "react-dom": "^18",
    "next": "^15.5.10"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "eslint-plugin-security": "^2.1.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1"
  }
}